  <!-- EN: electricity -->
  <Electricity.label>điện năng</Electricity.label>
  <!-- EN: Harness the power of electricity for a hundred different tasks. -->
  <Electricity.description>Khai thác sức mạnh của điện cho một trăm tiến trình khác nhau.</Electricity.description>
