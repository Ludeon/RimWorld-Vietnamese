  <SmokepopBeltRadius.description></SmokepopBeltRadius.description>
  <!-- {0} c -->
  <SmokepopBeltRadius.formatString></SmokepopBeltRadius.formatString>

  <!-- equip delay -->
  <EquipDelay.label></EquipDelay.label>
  <!-- The time it takes to equip and unequip this item. -->
  <EquipDelay.description></EquipDelay.description>
  <!-- {0} s -->
  <EquipDelay.formatString></EquipDelay.formatString>


